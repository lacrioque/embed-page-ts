{
  "name": "embed-page-ts",
  "version": "1.0.0",
  "module": "dist/es6/index.js",
  "main": "dist/commonjs/index.js",
  "types": "dist/types",
  "type": "module",
  "author": "Markus Fl√ºr <markus.fluer@syncomnet.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "build:es6": "tsc -p tsconfig.json --target esnext --outDir dist/es6",
    "build:cjs": "tsc -p tsconfig.json --target es2016 --module commonjs --outDir dist/commonjs",
    "prebuild": "yarn lint",
    "build": "run-p build:es6 build:cjs"
  },
  "dependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "npm-run-all": "^4.1.5"
  }
}
